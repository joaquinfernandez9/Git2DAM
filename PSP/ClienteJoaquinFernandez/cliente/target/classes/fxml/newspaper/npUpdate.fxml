    <TableView fx:id="tableNews" layoutX="66.0" layoutY="57.0" prefHeight="386.0" prefWidth="406.0">
        <columns>
            <TableColumn fx:id="idColumn" prefWidth="75.0" text="ID" />
            <TableColumn fx:id="nameColumn" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="dateColumn" prefWidth="75.0" text="Price" />
        </columns>
    </TableView>
    <Button layoutX="549.0" layoutY="248.0" mnemonicParsing="false" onAction="#update" text="Add" />
    <TextField fx:id="nameLabel" layoutX="494.0" layoutY="189.0" promptText="Name" />
