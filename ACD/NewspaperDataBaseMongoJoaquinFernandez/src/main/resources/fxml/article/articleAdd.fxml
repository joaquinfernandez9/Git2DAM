        </columns>
    </TableView>
    <Button layoutX="554.0" layoutY="225.0" mnemonicParsing="false" onAction="#add" text="add" />
    <MFXTextField fx:id="name" layoutX="498.0" layoutY="49.0" prefHeight="37.0" prefWidth="176.0" promptText="Title" />
