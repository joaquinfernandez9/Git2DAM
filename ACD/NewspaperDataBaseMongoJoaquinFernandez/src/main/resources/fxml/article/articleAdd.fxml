            <TableColumn fx:id="titleColumn" prefWidth="75.0" text="Title" />
            <TableColumn fx:id="typeIDColumn" prefWidth="75.0" text="Type ID" />
        </columns>
    </TableView>

    <TableView fx:id="npTable" layoutX="50.0" layoutY="74.0" prefHeight="156.0" prefWidth="406.0">
        <columns>
            <TableColumn fx:id="idTable" prefWidth="100.0" text="Id" />
