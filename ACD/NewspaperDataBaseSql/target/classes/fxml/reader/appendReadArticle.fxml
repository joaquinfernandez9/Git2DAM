<?xml version="1.0" encoding="UTF-8"?>

<?import io.github.palexdev.materialfx.controls.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="500" prefWidth="700" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ui.pantallas.reader.appendArticle.AppendArticleController">
    <TableView fx:id="readersTable" layoutX="23.0" layoutY="35.0" prefHeight="188.0" prefWidth="406.0">
        <columns>
            <TableColumn fx:id="idColumn" prefWidth="75.0" text="ID" />
            <TableColumn fx:id="nameColum" prefWidth="75.0" text="Name" />
            <TableColumn fx:id="dateColumn" prefWidth="75.0" text="Date of Birth" />
        </columns>
    </TableView>
    <TableView fx:id="subscriptionsTable" layoutX="23.0" layoutY="235.0" prefHeight="236.0" prefWidth="406.0">
        <columns>
            <TableColumn fx:id="idSubscription" prefWidth="75.0" text="ID Subscription" />
            <TableColumn fx:id="idReader" prefWidth="75.0" text="ID Reader" />
            <TableColumn fx:id="idNewspaper" prefWidth="75.0" text="ID Newspaper" />
            <TableColumn fx:id="singDate" prefWidth="75.0" text="Sing date" />
            <TableColumn fx:id="cancellationDate" prefWidth="75.0" text="Cancellation date" />
        </columns>
    </TableView>
   <Button layoutX="560.0" layoutY="431.0" mnemonicParsing="false" onAction="#addReadArticle" text="Button" />
    <MFXTextField fx:id="ratingText" layoutX="544.0" layoutY="295.0" promptText="Rating" />
    <MFXTextField fx:id="idArticleText" layoutX="544.0" layoutY="353.0" promptText="ID Article" />

</AnchorPane>
