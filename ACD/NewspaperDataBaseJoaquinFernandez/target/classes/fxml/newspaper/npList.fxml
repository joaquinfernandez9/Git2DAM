                <TableColumn fx:id="dateColumn" prefWidth="75.0" text="Price" />
            </columns>
        </TableView>
        <TableView fx:id="articleTable" layoutX="443.0" layoutY="57.0" prefHeight="386.0" prefWidth="227.0">
