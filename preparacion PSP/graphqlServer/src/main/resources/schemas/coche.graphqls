type Coche{
    id: String
    marca: String
    ruedas: [Rueda]
}

type Rueda{
    nombre: String
    pulgadas: Int
    id_coche: String
}

type Query {
    getAllCoches: [Coche]
    getCoche(idCoche: String!): Coche
}

type Mutation {
    postCoche(id: String!, marca: String!): Coche
    addRueda(id: String!, nombre: String!, pulgadas: Int!, idCoche: String!): Coche
    deleteCoche(idCoche: String!): Boolean
}